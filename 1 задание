#include <iostream>
#include <cmath>

using namespace std;

int main()
{
    setlocale(LC_ALL, "RUS");
    int a, b, c;
    cout << " Введите первое число: ";
    cin >> a;

    cout << endl << " Введите второе число: ";
    cin >> b;

    cout << endl << " Введите третье число: ";
    cin >> c;
    if (a < 0, b < 0, c < 0)
    {
        cout << endl << " Отрицательные числа вводить нельзя " << endl;;
        return 0;
    }

    int s1, s2, s3, s4, s5, s6;

    s1 = a + b + c;
    s2 = a + b * c;
    s3 = a * b + c;
    s4 = (a + b) * c;
    s5 = a * (b + c);
    s6 = a * b * c;
    const int n = 6;
    int arr[n] = { s1,s2,s3,s4,s5,s6 };
    int k = arr[1];
    for (int i = 0; i < n; i++)
    {
        if (k < arr[i])
        {
            k = arr[i];
        }
    }
    cout << endl << " Наибольшее значение: " << k << endl;

 cout<<"Это находилось в ветке"<<endl;
сout<< "Это находится в ветке test0000000"<<endl;

}
